<% if (allrecs.length > 0) {%>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>City</th>
            <th>State</th>
            <th>Username</th>
        </tr>

      <% allrecs.forEach((recref, index) => { %>
        <tr>
            <td><%= recref.customer_id %></td>
            <td><%= recref.firstname %></td>
            <td><%= recref.lastname %></td>
            <td><%= recref.city %></td>
            <td><%= recref.state %></td>
            <td><%= recref.username %></td>
        </tr>
    <% }); %>
    </table>
    <% } else { %>
        <p>Not Records Available</p>
    <% } %>

